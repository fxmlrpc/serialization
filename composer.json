{
    "name": "fxmlrpc/serialization",
    "description": "Serialization logic for FXMLRPC",
    "license": "MIT",
    "keywords": ["xmlrpc", "xml", "rpc", "serialization"],
    "homepage": "http://fxmlrpc.io",
    "authors": [
        {
            "name": "Lars Strojny",
            "email": "lstrojny@php.net",
            "homepage": "http://usrportage.de"
        },
        {
            "name": "Márk Sági-Kazár",
            "email": "mark.sagikazar@gmail.com"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0"
    },
    "require-dev": {
        "ext-xmlrpc": "*",
        "ext-xmlreader": "*",
        "ext-xmlwriter": "*",
        "zendframework/zendframework1": "^1.12.4",
        "zendframework/zend-xmlrpc": "^2.3.2",
        "phpspec/phpspec": "^3.0",
        "henrikbjorn/phpspec-code-coverage" : "^3.0",
        "phpbench/phpbench": "^0.8",
        "behat/behat": "^3.0",
        "webmozart/assert": "^1.0",
        "phpunit/phpunit": "^5.5"
    },
    "suggest": {
        "ext-xmlrpc": "Used in native serialization logic",
        "ext-xmlreader": "Used in SAX serialization logic",
        "ext-xmlwriter": "Used in SAX serialization logic",
        "zendframework/zendframework1": "Used in Zend 1 serialization wrapper",
        "zendframework/zend-xmlrpc": "Used in Zend 2 serialization wrapper"
    },
    "autoload": {
        "psr-4": {
            "Fxmlrpc\\Serialization\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fxmlrpc\\Benchmark\\": "benchmarks/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpspec run && vendor/bin/behat",
        "test-ci": "vendor/bin/phpspec run -c phpspec.ci.yml && vendor/bin/behat"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
